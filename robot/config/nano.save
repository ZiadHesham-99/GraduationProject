
# Global options
fixed_frame: "base_link"
background_color: [0.0, 0.0, 0.0, 1.0]

# Add a TF display
- name: "TF"
  enabled: true

# Add an InteractiveMarkers display
- name: "InteractiveMarkers"
  enabled: true

# Add an IMU display
- name: "IMU"
  enabled: true
  topic: "/imu_topic"  # Replace with the actual topic publishing IMU data

# Add a Temperature display
- name: "Temperature"
  enabled: true
  topic: "/temperature_topic"  # Replace with the actual topic publishing temperature data

# Add an MQ7 display
- name: "MQ7"
  enabled: true
  topic: "/mq7_topic"  # Replace with the actual topic publishing MQ7 data

# Add a Gyroscope display
- name: "Gyroscope"
  enabled: true
  topic: "/gyroscope_topic"  # Replace with the actual topic publishing gyroscope data

