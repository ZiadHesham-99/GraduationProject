<launch>

    <include file="$(find gazebo_ros)/launch/empty_world.launch">
    		<arg name="paused" value="true"/>
    </include>


  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find robot_control)/config/config.yaml" command="load"/>


    <param name="robot_description" command="cat $(find robot)/urdf/fire.urdf" />

    <node name="spawn_robot" pkg="gazebo_ros" type="spawn_model" respawn= "false" output="screen"
		args="-urdf -model manipulator -param robot_description -z 0.0"/>
  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="/manipulator" args="left_wheel_effort_controller right_wheel_effort_controller joint_state_controller"/>

    <node name= "differential_drive_controller" pkg="robot_control" type= "DifferentialDriveController.py" output="screen"/>
  

            
</launch>
